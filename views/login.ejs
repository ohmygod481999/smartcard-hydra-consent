                      action="<%= action %>"
                      method="POST"
                    class="contact-form animate__animated animate__fadeInLeft animate__delay-2s"
                      id="contact-form">
                      <div class="row">
                        <input
                          type="hidden" name="_csrf" value="<%= csrfToken %> ">
                        <input
                          type="hidden" name="challenge" value="<%= challenge %> ">
                        <div class="col-12 col-md-12 form-group">
                          <input class="form-control" id="email"
                            type="email" name="email" placeholder="Tài khoản" required="">
                        </div>
                        <div class="col-12 col-md-12 form-group">
                          <input class="form-control" id="password"
                            type="password" name="password" placeholder="Mật khẩu" required="">
                        </div>
                        <div class="col-12 col-md-12 form-group">
                          <div class="form-check">
                            <input class="form-check-input" type="checkbox" value="1" id="remember" name="remember">
                            <label class="form-check-label" for="flexCheckDefault">
                              Ghi nhớ đăng nhập
                            </label>
                          
                        </div>

                        </div>
                        
                        <% if (typeof(error) !== "undefined") { %>
                          <div class="invalid-feedback d-block text-center mb-3">
                            <%= error %>
                          </div>
                        <% } %>
                        
                        <div class="col-12 mb-4 form-submit">
                          <input class="clickbtn button-main button-scheme" type="submit" id="accept" name="submit" value="Đăng nhập" />
                          <!-- <button class="clickbtn button-main button-scheme"
                            id="contact-submit" type="submit">Đăng nhập</button> -->
                        </div>
                      </div>
